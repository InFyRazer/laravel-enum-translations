{
    "name": "ifresh/laravel-enum-translations",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/translation": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "IFresh\\EnumTranslations\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tom Kuijer",
            "email": "t.kuijer@ifresh.nl"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "IFresh\\EnumTranslations\\EnumTranslationsServiceProvider"
            ],
            "aliases": {
                "EnumTranslator": "IFresh\\EnumTranslations\\EnumTranslatorFacade"
            }
        }
    }
}
